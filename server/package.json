{
  "name": "upvibe-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:docker": "npx tsc -p tsconfig.dev.json && npx tsc-alias -p tsconfig.dev.json && node --inspect=0.0.0.0:9229 build_debug/server.js",
    "test:file": "nodemon --exec ts-node -r tsconfig-paths/register ./tests/unit/app/app.test.ts",
    "test": "jest --watch --config jest.config.ts",
    "preitter": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "Volodymyr Fihurniak",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.9.1",
    "npm": "^9.8.1",
    "pg": "^8.11.1",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@jest/globals": "^29.6.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.3",
    "@types/pg": "^8.10.2",
    "jest": "^29.6.1",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
