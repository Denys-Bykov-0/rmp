{
  "name": "upvibe-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && tsc-alias -p tsconfig.json",
    "dev:build": "npx tsc -p tsconfig.dev.json && npx tsc-alias -p tsconfig.dev.json",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:debug:docker": "NODE_ENV=development DEBUG='*' nodemon -L --inspect=0.0.0.0:9229 src/server.ts",
    "dev:build:docker": "npx tsc -p tsconfig.dev.json && npx tsc-alias -p tsconfig.dev.json && cp -r src/sql buildDebug/sql",
    "dev:start:docker": "node buildDebug/server.js",
    "test": "jest --watch --coverage --config jest.config.ts",
    "lint": "eslint --fix \"src/**/*.ts\" && tsc --noEmit",
    "lint:prettier": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "Volodymyr Fihurniak",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.9.1",
    "npm": "^9.8.1",
    "pg": "^8.11.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.5",
    "@jest/globals": "^29.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/amqplib": "^0.10.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
