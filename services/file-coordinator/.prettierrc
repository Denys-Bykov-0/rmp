{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "endOfLine": "lf",
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-sql"],
  "importOrderParserPlugins": ["typescript"],
  "overrides": [
    {
      "files": "*.sql",
      "options": {
        "language": "postgresql"
      }
    }
  ]
}
